#include<iostream>
#include<string>
using namespace std;

int priority(string designation){
    if (designation == "CEO") return 1;
    if (designation == "CTO") return 2;
    if (designation == "CFO") return 3;
    if (designation== "VP")  return 4;
    if (designation== "MGR") return 5;
    if (designation == "EMP") return 6;
     return 999;}

    void insertionSort(string arr[], int n) {
    for (int i = 1; i < n; i++) {
    string key = arr[i];
    int keypriority = priority(key);
    int j = i - 1;
while (j >= 0 && priority(arr[j]) > keypriority) {
    arr[j + 1] = arr[j];
    j--;}
    arr[j + 1] = key;}}

int main() {
    string employees[] = {"EMP","CFO","MGR","EMP","VP","CTO","MGR","CEO"};
    int number = 8;
    cout << "Before Sorting"<<endl;
    for (int i = 0; i < n; i++)
    cout << employees[i] << " ";

    insertionSort(employees, number);
    cout << "After Sorting by Priority:"<<endl;
    for (int x = 0; x < n; x++)
        cout << employees[x] << " ";
    cout << "\n";

    return 0;
}
